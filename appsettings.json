{
  "Logging": {
    "Logging": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "RunOptions": {
    // Values[0 = TFN Algorithim, 1= Always True, 2 = Always False], if not provided Default is 0
    "TFNValidator": "0",
    // Number of seconds to store cached values, if not provided default is 30 seconds.
    "CacheSeconds": "30",
    // The number of patterns required to link a detection, If not  provided its set to 3.
     "MaxRequiredCacheDetections": "3"
  }
}
